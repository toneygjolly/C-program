

#include<stdio.h>
int main(){
    int n,i,a[10];
    printf("enter the limit\n");
    scanf("%d",&n);
    printf("limit=%d\n",n);
    printf("enter the array\n");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
     printf("array=:");
     for(i=0;i<n;i++){
        printf("%d\t",a[i]);
    }
a[1]=12;
a[2]=21;
 printf("\nchanged array:=");
     for(i=0;i<n;i++){
        printf("%d\t",a[i]);
    }
    return 0;
}
